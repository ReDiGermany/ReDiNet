<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Text?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import com.github.redigermany.redinet.view.Bookmark?>
<?import com.github.redigermany.redinet.view.NavigationButton?>
<?import javafx.scene.web.WebView?>
<?import com.github.redigermany.redinet.view.WebTab?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            prefHeight="720.0" prefWidth="1280.0" stylesheets="@style.css"
            styleClass="root">
<VBox>
    <HBox viewOrder="-Infinity" style="-fx-padding: 10 10 0 10;">
        <NavigationButton fx:id="prevBtn" imageUrl="arrow-left-solid.png" />
        <NavigationButton fx:id="forwBtn" imageUrl="arrow-right-solid.png" />
        <NavigationButton fx:id="reloadBtn" imageUrl="arrow-reload-solid.png" />
        <TextField viewOrder="-Infinity" fx:id="urlBar" styleClass="urlBar">URL</TextField>
        <NavigationButton fx:id="goBtn" imageUrl="directions-solid.png" />
        <NavigationButton fx:id="homeBtn" imageUrl="home-solid.png" />
        <NavigationButton fx:id="bookmarkBtn" imageUrl="star-empty-solid.png" />
        <NavigationButton fx:id="menuBtn" imageUrl="menu-solid.png" />
    </HBox>
    <HBox fx:id="bookmarks"/>
    <TabPane fx:id="tabPane">
        <WebTab closable="false" image="plus-solid.png" fx:id="newTab" />
    </TabPane>
    <HBox fx:id="infoBar">
        <ProgressBar fx:id="progressBar" />
        <Label fx:id="progressLabel" />
    </HBox>
</VBox>
</AnchorPane>
